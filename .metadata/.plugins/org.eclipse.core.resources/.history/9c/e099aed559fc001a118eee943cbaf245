<?php

class Person
{
    private $name;
    private $age;
    private $username;
    private $password;
    
    public function __construct($name) {
        $this->name = $name;
        echo "Hello my name is " .$name . "<br>";
        $this->username = "isaiah";
        $this->password = "password";
    }
    
}
?>